.data
n: .word 10
.text
la t0, n
lw a2, 0(t0)
jal Fibonacci
j Exit

Fibonacci:



li t3, 1
bge a2, zero, Base2
li a0, 0
j End

Base2:
bne a2, t3, Recurse
li a0, 1
j End

Recurse:
addi sp, sp, -12
addi a2, a2, -1
sw a2, 0(sp)
sw ra, 4(sp)
jal Fibonacci
sw a0, 8(sp) 
lw a2, 0(sp) 
addi a2, a2, -1
jal Fibonacci
lw t4, 8(sp)
add a0, a0, t4
lw ra, 4(sp)
addi sp, sp, 12
End:
jr ra, 0
Exit:
