.data
a : .word 3
b : .word 10
c : .word 20

.text

li t3, 3
sw t3, 0(sp)
jal Fibonacci

Fibonacci:
lw a2, 0(sp)
bne a2, zero, ElseIf
li a0, 0
j End

ElseIf:
li t3, 1
bne a2, t3, Else
li a0, 1
j End

Else:
addi sp, sp, -12
sw a0, 0(sp)
addi a2, a2, -1
sw a2, 4(sp)
addi a2, a2, -1
sw a2, 8(sp)


End:
jr ra, 0

